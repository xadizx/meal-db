<%= link_to meal_path(@meal_thumbnail.external_id), class: "group" do %>
  <%= image_tag @meal_thumbnail.thumbnail_image_url, class: "aspect-square w-full rounded-lg bg-gray-200 object-cover group-hover:opacity-75" %>
  <h2 class="mt-4 text-sm text-gray-700"><%= @meal_thumbnail.category_name %></h2>
  <h3 class="mt-1 text-lg font-medium text-gray-900"><%= @meal_thumbnail.name %></h3>
<% end %>
